msgid "set to %s"
msgstr "establece en %s"

#: models.py:578
#, python-format
msgid "changed from \"%(old_value)s\" to \"%(new_value)s\""
msgstr "cambiado de \"%(old_value)s\" a \"%(new_value)s\""

